{
    "name": "starter-file",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "compile-sass": "node-sass -w sass -o dist/css",
        "concat": "concat -o dist/css/style.css dist/css/navbar.css dist/css/header.css dist/css/section1.css dist/css/section2.css dist/css/section3.css dist/css/section4.css  dist/css/footer.css dist/css/protege.css",
        "prefix": "postcss dist/css/style.css --use autoprefixer -d dist/css/prefixed",
        "build": "npm-run-all compile-sass  prefix concat"
    },
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "autoprefixer": "^9.7.5",
        "concat": "^1.0.3",
        "postcss-cli": "^7.1.0"
    },
    "dependencies": {
        "node-sass": "^4.13.1",
        "npm-run-all": "^4.1.5"
    }
}